{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build robot manipulator",
            "type": "shell",
            "command": "cl",
            "args": [
                "/EHsc",
                "main_new.cpp",
                "Camera.cpp",
                "ElementaryMove.cpp",
                "Gripper.cpp",
                "Manipulator.cpp",
                "MovableLink.cpp",
                "Movement.cpp",
                "Tests.cpp",
                "/Fe:Robot-manipulator.exe"

            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "problemMatcher": [
                "$msCompile"
            ]
        },
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\${fileBasenameNoExtension}.exe",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ]
}